{
  "name": "n4js-node",
  "author": "NumberFour AG <npm@numberfour.eu>",
  "license": "EPL-1.0",
  "repository": "https://github.com/NumberFour/n4js-libs",
  "description": "N4JS node.js runtime environment.",
  "keywords": [
    "n4js",
    "es6",
    "es2015",
    "transpiler",
    "ide",
    "n4jside",
    "n4js-ide"
  ],
  "engines": {
    "node": ">= 5.0.0"
  },
  "bin": {
    "n4js": "./src-gen/n4js-cli.js"
  },
  "main": "./src-gen/index.js",
  "browser": "./src-gen/browser.js",
  "react-native": "./src-gen/react-native.js",
  "dependencies": {
    "n4js-es5": "latest",
    "node-fetch": "^1.6.3",
    "systemjs": "^0.19.39"
  },
  "files": [
    "manifest.n4mf",
    "src-gen"
  ],
  "n4js_manifest": {
    "ProjectId": "n4js-node",
    "VendorId": "org.eclipse.n4js",
    "VendorName": "Eclipse N4JS Project",
    "ProjectType": "runtimeEnvironment",
    "ProjectVersion": "0.3.3",
    "ExtendedRuntimeEnvironment": "n4js-es5",
    "ProvidedRuntimeLibraries": [
      "n4js-runtime-n4",
      "n4js-runtime-v8",
      "n4js-runtime-es2015",
      "n4js-runtime-esnext",
      "n4js-runtime-fetch",
      "n4js-runtime-node",
      "n4js-runtime-ecma402"
    ],
    "Output": "src-gen",
    "Sources": {
      "external": [
        "\"src/js\""
      ]
    },
    "ExecModule": "n4js-cli",
    "ModuleFilters": {
      "noModuleWrap": [
        "\"**\""
      ]
    }
  },
  "version": "0.1.1"
}
